<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC
	"-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="com.ohmija.repository.DataDAO">
	<insert id="saveData" parameterType="data">
		insert into festival_status
			values(#{idx}, #{region}, #{basic_organization_name},
					#{fes_name}, #{fes_category}, #{fes_date},
					#{fes_place}, #{hosting_method}, #{first_hosting_year},
					#{visitor_count}, #{foriegner_count}, #{dedicated_organization}, #{organization_type})
	</insert>
</mapper>